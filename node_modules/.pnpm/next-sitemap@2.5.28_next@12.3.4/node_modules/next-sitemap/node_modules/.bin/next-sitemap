#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/goutham/Goutham.portfolio/node_modules/.pnpm/next-sitemap@2.5.28_next@12.3.4/node_modules/next-sitemap/bin/node_modules:/Users/goutham/Goutham.portfolio/node_modules/.pnpm/next-sitemap@2.5.28_next@12.3.4/node_modules/next-sitemap/node_modules:/Users/goutham/Goutham.portfolio/node_modules/.pnpm/next-sitemap@2.5.28_next@12.3.4/node_modules:/Users/goutham/Goutham.portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/goutham/Goutham.portfolio/node_modules/.pnpm/next-sitemap@2.5.28_next@12.3.4/node_modules/next-sitemap/bin/node_modules:/Users/goutham/Goutham.portfolio/node_modules/.pnpm/next-sitemap@2.5.28_next@12.3.4/node_modules/next-sitemap/node_modules:/Users/goutham/Goutham.portfolio/node_modules/.pnpm/next-sitemap@2.5.28_next@12.3.4/node_modules:/Users/goutham/Goutham.portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/next-sitemap" "$@"
else
  exec node  "$basedir/../../bin/next-sitemap" "$@"
fi
